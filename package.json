{
  "name": "vue-multiple-pages",
  "version": "1.0.0",
  "description": "A multiple pages templates by use vue-router and vuex.",
  "main": "app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "pro": "cross-env NODE_ENV=production webpack --config webpack.config.js"
  },
  "author": "yinzhigang",
  "license": "ISC",
  "env": [
    "development",
    "production"
  ],
  "runEnv": "development",
  "devServer": {
    "contentBase": "dist",
    "historyApiFallback": true,
    "inline": true,
    "port": 8888,
    "hot": true,
    "open": false
  },
  "devEntry": {
    "index": "./app-index/index.js",
    "video": "./app-video/video.js"
  },
  "proEntry": {
    "index": "./app-index/index.js"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "webpack": "^3.5.4",
    "webpack-dev-server": "^2.7.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "file-loader": "^2.0.0",
    "url-loader": "^1.1.2",
    "css-loader": "^1.0.1",
    "style-loader": "^0.23.1",
    "html-webpack-plugin": "^3.2.0",
    "clean-webpack-plugin": "^0.1.16",
    "cross-env": "^5.2.0",
    "vue-loader": "^13.3.0",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "sass-loader": "^7.0.1",
    "node-sass": "^4.10.0"
  }
}